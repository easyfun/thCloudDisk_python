/*==========ThTitleBar==========*/
QFrame#ThTitleBar{
	color:black;
	background-color:purple;
}

QFrame#ThTitleBar *{
	color:black;
	background-color:purple;
	border-style:none;
}

QFrame#ThTitleBar QToolButton:hover{
	background-color:rgb(0,0,192);
}

QFrame#ThTitleBar QToolButton#closeApplication:hover{
	background-color:red;
}